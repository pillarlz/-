            .topWrapper {
                position: relative;
                width: 100%;
                height: 130px;
                display: -webkit-box;
                display: -webkit-flex;
                margin-bottom: 10px;
            }
            
            .topWrapper .mask {
                width: 100%;
                height: 100%;
            }
            
            .topWrapper .mask img {
                width: 100%;
                height: 100%;
                -webkit-filter: blur(5px);
            }
            
            .topWrapper .topContent {
                padding-left: 20px;
                position: absolute;
                left: 0;
                top: 0;
                height: 90px;
                padding-top: 20px;
            }
            
            .topWrapper .topContent .imgWrapper {
                width: 90px;
                height: 90px;
            }
            
            .topWrapper .topContent .imgWrapper img {
                width: 100%;
                border-radius: 100%;
            }
            
            .topWrapper .topContent {
                display: -webkit-box;
                display: -webkit-flex;
            }
            
            .topContent_text {
                -webkit-box-flex: 1;
                -webkit-flex: 1;
                float: left;
                margin-left: 20px;
                line-height: 30px;
            }
            
            .topContent_text h1 {
                display: -webkit-box;
                overflow: hidden;
                text-overflow: ellipsis;
                -webkit-line-clamp: 1;
                -webkit-box-orient: vertical;
                font-size: 16px;
                color: #fff;
            }
            
            .topContent_text p {
                font-size: 12px;
                color: #fff;
            }
            
            .topContent_text p span.iconfont {
                font-size: 12px;
            }
            
            .aui-list-view-cell span.iconfont {
                margin: 0 5px;
            }
            
            .aui-list-view-cell a {
                color: #444;
            }
            
            .aui-list-view-cell p {
                margin-top: 5px;
            }
            
            .aui-list-view-cell li {
                width: 20%;
                float: left; text-align: center;
            }
            
            .aui-list-view-cell li img {
                width: 70%;

            }
        </style>
